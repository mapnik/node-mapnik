<Map srs="epsg:3857">
    <Style name="style">
        <Rule>
            <RasterSymbolizer scaling="sinc" filter-factor="2.0"/>
        </Rule>
    </Style>
    <Layer name="raster" srs="epsg:3857">
        <StyleName>style</StyleName>
        <Datasource>
            <Parameter name="file">./images/sat_image.png</Parameter>
            <Parameter name="type">gdal</Parameter>
        </Datasource>
    </Layer>

</Map>
