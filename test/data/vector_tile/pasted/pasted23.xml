<?xml version="1.0" encoding="utf-8"?>
<Map srs="epsg:3857" background-color="steelblue">

    <Style name="style">
        <Rule>
            <PolygonSymbolizer fill="white" clip="false"/>
        </Rule>
    </Style>

    <Layer name="pasted" buffer-size="8" srs="epsg:3857">
        <StyleName>style</StyleName>
        <Datasource>
            <Parameter name="file">pasted23.shp</Parameter>
            <Parameter name="encoding">iso-8859-1</Parameter>
            <Parameter name="type">shape</Parameter>
        </Datasource>
    </Layer>

</Map>
