            <RasterSymbolizer opacity=".7" scaling="bilinear"/>
        </Rule>
    </Style>
    <Layer name="raster" srs="epsg:3857">
        <StyleName>style</StyleName>
        <StyleName>style2</StyleName>
    </Layer>
</Map>
