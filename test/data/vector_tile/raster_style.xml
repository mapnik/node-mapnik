<Map srs="epsg:3857">
    <Style name="style" image-filters="agg-stack-blur(1,1) sharpen()">
        <Rule>
            <RasterSymbolizer scaling="bilinear"/>
        </Rule>
    </Style>
    <Style name="style2" comp-op="soft-light" opacity=".7" image-filters="agg-stack-blur(1,1) sharpen()">
